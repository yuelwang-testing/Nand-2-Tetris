/** 
 * Multiplexor:
 * if (sel = 0) out = a, else out = b
 */
CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Not(in = sel, out = unSel);
    And(a = a, b = unSel, out = c);
    And(a = sel, b = b, out = d);
    Or(a = c, b = d, out = out);
}