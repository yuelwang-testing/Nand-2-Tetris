/**
 * Or gate:
 * if (a or b) out = 1, else out = 0 
 */
CHIP Or {
    IN a, b;
    OUT out;

    PARTS:
    Not(in = a, out = c);
    Not(in = b, out = d);
    Nand(a = c, b = d, out = out);
}